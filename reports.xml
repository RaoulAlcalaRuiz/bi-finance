    <template id="report_ca_template">
         <t t-call="web.html_container">
             <h2>Id year</h2>
             <t t-esc="doc_ids"/>

             <br/>
             <h2>Months</h2>
             <t t-foreach="months" t-as="id">
                    <t t-esc="id.id"/>
             </t>
             <br/>
             <t t-esc="months_ids"/>

             <br/>
             <h2>Employee</h2>
             <t t-foreach="employees" t-as="employee">
                    <t t-esc="employee.commercial_id.id"/>
             </t>
             <br/>
             <t t-esc="employees_ids"/>

             <br/>
             <h2>Real sales</h2>
             <t t-esc="real_annual_sales"/>

             <br/>
             <h2>Goal Sales</h2>
             <t t-esc="goal_annual_sales"/>
         </t>
    </template>

</odoo>
